    <div class="body-main">
        <div class="conBox">
        <div class="container">
            <!--<form action="#">-->
            <form action="/contact" method="POST">
                <div class="f-name-table">
                <div class="f-name-fieled">
                    <div class="f-name-label">
                        <label for="fname">First Name:</label>
                        <div class="f-name-fname">my Name</div>
                    </div>
                    <div class="f-name-input">
                        <input type="text" id="fname" name="firstname" placeholder="Change first name.."><br>
